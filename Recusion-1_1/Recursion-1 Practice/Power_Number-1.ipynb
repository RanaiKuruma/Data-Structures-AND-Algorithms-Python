    "def pow_n(x, n):\n",
    "    if(n == 0):\n",
    "        return 1\n",
    "    else:\n",
    "        return x * pow_n(x, n-1)    \n",
    "\n",
    "x = int(input())\n",
    "n = int(input())        \n",
    "\n",
    "print(pow_n(x, n))\n"
   ]
